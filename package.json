    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "amqp-connection-manager": "^4.1.2",
    "amqplib": "^0.8.0"
